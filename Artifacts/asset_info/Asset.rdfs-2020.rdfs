      <cims:AssociationUsed>Yes</cims:AssociationUsed>
      <cims:inverseRoleName rdf:resource="#TransformerEndInfo.EnergisedEndNoLoadTests"/>
      <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
      <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Transformer end that current is applied to in this no-load test.</rdfs:comment>
      <rdfs:domain rdf:resource="#NoLoadTest"/>
      <rdfs:label xml:lang="en">EnergisedEnd</rdfs:label>
      <rdfs:range rdf:resource="#TransformerEndInfo"/>
   </rdf:Description>
